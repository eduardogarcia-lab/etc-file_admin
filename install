mv file_admin.service /lib/systemd/system/
cp templates/plantilla_torrent.txt /var/www/filegator/repository/
chmod 644 /var/www/filegator/repository/plantilla_torrent.txt
mkdir -p /var/log/file_admin/
chmod 755 /var/log/file_admin/
mkdir -p /var/www/filegator/torrents/
chmod 775 /var/www/filegator/torrents/
mkdir -p /var/www/filegator/torrents_map_folder/
chmod 775 /var/www/filegator/torrents_map_folder/
systemctl enable file_admin
systemctl start file_admin
